{
  "version": 2,
  "name": "cracken-auth",

  "routes": [
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/callback",
      "dest": "/api/callback.js"
    },
    {
      "src": "/discord-callback",
      "dest": "/api/discord-callback.js"
    }
  ],
  "env": {
    "GOOGLE_CLIENT_ID": "@google_client_id",
    "GOOGLE_CLIENT_SECRET": "@google_client_secret"
  },
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}
