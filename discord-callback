// discord-callback.js
// Redirection vers le vrai callback Discord dans /api/

module.exports = async function handler(req, res) {
  // Rediriger vers le vrai callback avec tous les paramÃ¨tres
  const queryString = new URLSearchParams(req.query).toString();
  const redirectUrl = `/api/discord-callback?${queryString}`;
  
  console.log('ðŸ”„ Redirection callback Discord:', redirectUrl);
  
  return res.redirect(302, redirectUrl);
};
